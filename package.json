{
  "name": "metravel",
  "main": "./entry.js",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "check-deps": "node scripts/check-dependencies.js",
    "check:image-architecture": "node scripts/check-image-architecture.js",
    "test:cors": "node scripts/test-cors-proxy.js",
    "start": "npx expo start",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "web:prod": "NODE_ENV=production npx expo start --web",
    "e2e": "node scripts/e2e-run.js",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "test": "jest --watchAll",
    "test:run": "jest --passWithNoTests --runInBand",
    "test:run:mem": "NODE_OPTIONS=--max-old-space-size=8192 jest --passWithNoTests --runInBand",
    "test:smoke:critical": "node scripts/run-smoke-critical.js",
    "test:smoke:critical:ci": "node scripts/run-smoke-critical.js --ci --output-file test-results/jest-smoke-results.json",
    "smoke:baseline:recommend": "node scripts/recommend-smoke-baseline.js --dir test-results --limit 7",
    "smoke:suite-baseline:recommend": "node scripts/recommend-smoke-suite-baseline.js --file test-results/quality-summary.json --format json",
    "smoke:suite-baseline:validate": "node scripts/validate-smoke-suite-baseline-recommendation.js --file test-results/smoke-suite-baseline-recommendation.json",
    "smoke:suite-baseline:validate:json": "node scripts/validate-smoke-suite-baseline-recommendation.js --file test-results/smoke-suite-baseline-recommendation.json --json",
    "ci:incident:template": "node scripts/generate-ci-incident.js",
    "ci:incident:publish": "node scripts/publish-ci-incident-snippet.js --summary-file test-results/quality-summary.json",
    "ci:incident:publish:json": "node scripts/publish-ci-incident-snippet.js --summary-file test-results/quality-summary.json --json",
    "ci:incident:validate": "node scripts/validate-ci-incident-snippet.js --file test-results/ci-incident-snippet.md",
    "ci:incident:validate:json": "node scripts/validate-ci-incident-snippet.js --file test-results/ci-incident-snippet.md --json",
    "ci:incident:payload:validate": "node scripts/validate-ci-incident-payload.js --file test-results/ci-incident-payload.json",
    "ci:incident:payload:validate:json": "node scripts/validate-ci-incident-payload.js --file test-results/ci-incident-payload.json --json",
    "ci:workflow:contract:validate": "node scripts/validate-ci-smoke-workflow-contract.js",
    "ci:workflow:contract:validate:json": "node scripts/validate-ci-smoke-workflow-contract.js --json",
    "ci:workflow:contract:summarize": "node scripts/summarize-ci-smoke-workflow-contract-validation.js --file test-results/ci-smoke-workflow-contract-validation.json",
    "validator:error-codes:docs:check": "node scripts/validate-validator-error-codes-doc-table.js --file docs/TESTING.md",
    "pr:ci-exception:validate:json": "node scripts/validate-pr-ci-exception.js --json",
    "quality:summary:validate": "node scripts/validate-quality-summary.js test-results/quality-summary.json",
    "test:watch": "jest --watch",
    "test:ci": "npm run check:image-architecture && jest --ci --runInBand --reporters=default --testLocationInResults --json --outputFile=jest-results.json | tee jest-full.log",
    "test:coverage": "jest --coverage",
    "config:diagnostics": "node scripts/runtime-config-diagnostics.js",
    "config:diagnostics:json": "node scripts/runtime-config-diagnostics.js --json",
    "config:diagnostics:strict": "node scripts/runtime-config-diagnostics.js --strict-warnings",
    "test:coverage:travel-details": "jest --coverage --runInBand TravelDetails --collectCoverageFrom=hooks/useTravelDetails*.ts --collectCoverageFrom=utils/travelDetails*.ts --collectCoverageFrom=components/travel/details/TravelDetailsContainer.tsx --collectCoverageFrom=components/travel/details/TravelDetailsHero.tsx --collectCoverageFrom=components/travel/details/TravelDetailsLazy.tsx --collectCoverageFrom=components/travel/details/TravelDetailsIcons.tsx",
    "format": "npx prettier --write .",
    "build": "npm run build:web",
    "build:web": "node ./node_modules/expo/bin/cli export -p web",
    "build:web:prod": "cp .env.prod .env && NODE_ENV=production EXPO_ENV=prod EXPO_WEB_BUILD_MINIFY=true EXPO_WEB_BUILD_GENERATE_SOURCE_MAP=false node ./node_modules/expo/bin/cli export --output-dir dist/prod -p web -c && node scripts/postprocess-rnw-styles.js dist/prod",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "lighthouse": "node scripts/run-lighthouse.js",
    "lighthouse:travel": "node scripts/lighthouse-travel-score.js",
    "lighthouse:travel:mobile": "LIGHTHOUSE_FORM_FACTOR=mobile node scripts/lighthouse-travel-score.js",
    "lighthouse:travel:desktop": "LIGHTHOUSE_FORM_FACTOR=desktop node scripts/lighthouse-travel-score.js",
    "lighthouse:produrl:travel:mobile": "node scripts/lighthouse-produrl.js --formFactor mobile --output ./lighthouse-report.produrl.mobile.json",
    "lighthouse:produrl:travel:desktop": "node scripts/lighthouse-produrl.js --formFactor desktop --output ./lighthouse-report.produrl.desktop.json",
    "lighthouse:produrl:summary": "node scripts/lighthouse-produrl-summary.js",
    "lighthouse:produrl:lcp": "node scripts/lighthouse-produrl-lcp.js",
    "test:travel:performance": "node scripts/test-travel-page-performance.js",
    "test:pages:performance": "node scripts/test-pages-performance.js",
    "e2e:perf-budget": "npx playwright test e2e/travel-details-perf-budget.spec.ts",
    "e2e:perf-budget:headed": "npx playwright test e2e/travel-details-perf-budget.spec.ts --headed",
    "clean": "npx expo start --clear",
    "reset": "EXPO_RESET_CACHE=true npx expo start --clear",
    "prod:web": "NODE_ENV=production node ./node_modules/expo/bin/cli export -p web",
    "postinstall": "node scripts/fix-lightningcss.js && (node ./node_modules/patch-package/dist/index.js || true)",
    "ios:prebuild": "./scripts/ios-prebuild.sh",
    "ios:build": "./scripts/ios-build.sh",
    "ios:submit": "./scripts/ios-submit.sh",
    "ios:build:dev": "eas build --platform ios --profile development",
    "ios:build:preview": "eas build --platform ios --profile preview",
    "ios:build:prod": "eas build --platform ios --profile production",
    "ios:submit:latest": "eas submit --platform ios --latest",
    "android:prebuild": "./scripts/android-prebuild.sh",
    "android:build": "./scripts/android-build.sh",
    "android:submit": "./scripts/android-submit.sh",
    "android:build:dev": "eas build --platform android --profile development",
    "android:build:preview": "eas build --platform android --profile preview",
    "android:build:prod": "eas build --platform android --profile production",
    "android:submit:latest": "eas submit --platform android --latest",
    "build:all:dev": "eas build --platform all --profile development",
    "build:all:preview": "eas build --platform all --profile preview",
    "build:all:prod": "eas build --platform all --profile production",
    "lint": "eslint . --max-warnings=0",
    "lint:ci": "mkdir -p test-results && eslint . --max-warnings=0 --format json --output-file test-results/eslint-results.json",
    "audit:high": "node scripts/yarn-audit-high.js",
    "perf:check": "./scripts/check-performance.sh",
    "perf:lighthouse": "npm run lighthouse:travel:mobile && npm run lighthouse:travel:desktop",
    "perf:bundle": "npm run build:web:prod && npm run analyze:bundle",
    "design:migrate": "node scripts/migrate-design-imports.js",
    "design:migrate:dry": "node scripts/migrate-design-imports.js --dry-run",
    "design:verify": "node scripts/verify-design-system.js",
    "test:seo:prod": "node scripts/test-seo-prod.js",
    "test:seo:prod:verbose": "node scripts/test-seo-prod.js --verbose",
    "release:check": "yarn run lint && ./verify-security-fixes.sh && yarn run audit:high && yarn run test:run && yarn run e2e && yarn run build:web",
    "validator:error-codes:docs:update": "node scripts/update-validator-error-codes-doc-table.js --file docs/TESTING.md",
    "validator:contracts:check": "jest --passWithNoTests --runInBand __tests__/scripts/validator-error-codes-centralization.test.ts __tests__/scripts/validator-error-codes-prefix-policy.test.ts __tests__/scripts/validator-error-codes-uniqueness.test.ts __tests__/scripts/validate-validator-error-codes-doc-table.test.ts __tests__/scripts/validate-validator-error-codes-policy.test.ts __tests__/scripts/validate-validator-contracts-summary.test.ts __tests__/scripts/summarize-validator-contracts-summary-validation.test.ts",
    "validator:contracts:summary": "node scripts/summarize-validator-contracts.js --json-output test-results/validator-contracts-summary.json",
    "validator:contracts:summary:validate": "node scripts/validate-validator-contracts-summary.js --file test-results/validator-contracts-summary.json"
  },
  "dependencies": {
