# Development Environment Variables для MeTravel iOS App

# Analytics Configuration
EXPO_PUBLIC_METRIKA_ID=test
EXPO_PUBLIC_GOOGLE_GA4=test
EXPO_PUBLIC_GOOGLE_API_SECRET=test

# Google OAuth Configuration
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here.apps.googleusercontent.com

# API Configuration
DEV_API_URL=http://localhost:3000
IS_LOCAL_API=true

# App Configuration
APP_ENV=development
API_TIMEOUT=30000

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_DEBUG_MODE=true

# Disable sharp-cli requirement (Expo will use alternative image processing)
EXPO_IMAGE_UTILS_NO_SHARP=1
